{
  "name": "LinkSender",
  "description": "Open tabs in other chrome sessions",
  "manifest_version": 2,
  "version": "0.6",
  "browser_action": {
      //"default_popup": "/html/popup.html"
  },
  "background": {
      "scripts": [ "/js/peer.js", "/js/background.js" ]
  },
  "permissions": [
      "background", "storage", "sessions", "tabs", "http://*/*", "https://*/*"
  ]
}