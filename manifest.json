{
  "name": "LinkSender",
  "description": "Open tabs in other chrome sessions",
  "version": "0.6",
  "browser_action": {
      "default_popup": "/html/popup.html"
  },
  "background": {
      "scripts": [ "/js/background.js" ]
  },
  "omnibox": { "keyword": "omnix" },
  "manifest_version": 2,
  "permissions": [
      "sessions", "tabs", "http://*/*", "https://*/*"
  ]
}